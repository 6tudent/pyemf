<html><head><title>$prog - $description</title>
<link href="layout.css" rel="stylesheet">
<link href="colors.css" rel="stylesheet">
</head>

<body>

	<div id="contentWrapper">
		<div id="header">
			<div id="titleLine">
				<span id="siteTitle">$prog</span>
				<span id="siteSubtitle"> - $description</span>
			</div>
		</div>
		<div id="mainMenu">
			$mainMenu
		</div>
		<div id="sidebar">
			<div id="sidebarOptions"></div>
			<div id="sidebarTabs"></div>
		</div>
		<div id="displayArea">
#if $htmlBody
			<div id="htmlBody">
				$htmlBody
			</div>
#end if
#if $preBody
			<div id="preBody">
<pre>
$preBody
</pre>
			</div>
#end if
		</div>
		<div id="footer">
			<div id="signatureLine">
				<span id="signatureLogo"><a href="http://sourceforge.net"><img src="http://sourceforge.net/sflogo.php?group_id=148144" width="88" height="31" border="0" alt="SourceForge Logo"></a></span>
				<span id="signatureAuthor">Author: $author &lt;robm at users.sourceforge.net&gt;</span>
				<span id="signatureDate">Last updated: $today</span>
			</div>
		</div>
	</div>
</body>

